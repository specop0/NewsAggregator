{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "Console": {
      "FormatterName": "simple",
      "FormatterOptions": {
        "TimestampFormat": "yyyy-MM-ddTHH:mm:ss ",
        "UseUtcTimestamp": true
      }
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndPoints": {
      "Http": {
        "Url": "http://newsaggregator:5010"
      }
    }
  },
  "PathBase": "/news",
  "Database": {
    "Url": "http://localrestserver:6491/data/NewsAggregator"
  },
  "Browser": {
    "UseIntegrated": false,
    "Url": "http://seleniumminer:5022/mine"
  }
}
